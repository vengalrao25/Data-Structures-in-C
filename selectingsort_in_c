
#include <stdio.h>

int main()
{
    int arr[] = {16,10,12,41,623,17} , min_index ,temp ;
    int size  = sizeof(arr)/sizeof(arr[0]);
    
    for(int i=0; i<size-1 ; i++)
    {
        int min_index= i ;
        for(int j=i+1 ; j<size ;j++)
        {
            
            if (arr[j]<arr[min_index])
            {
                min_index=j ;
            }
            
        }
        temp = arr[min_index];
        arr[min_index] =arr[i];
        arr[i]  = temp ; 
    }

    for(int i = 0 ; i<size ; i++)
    {
        printf("%d ",arr[i]);
    }
    return 0;
}
